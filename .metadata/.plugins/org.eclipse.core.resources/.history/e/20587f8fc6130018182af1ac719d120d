package exam;

import java.util.Scanner;

public class Main5 {
	public static void main(String[] args) {
		ball();
	}

	static double ball() {
		int r = 0;
		double obj = 0;
		double st = 4 / 3;

		Scanner scan = new Scanner(System.in);
		System.out.println("Podaj promień kuli");

		r = scan.nextInt();

		obj = st * Math.PI * Math.pow(r, 3);

		System.out.println("Objetość kuli dla promienia r = " + r + " wynosi " + obj);
		return 0;
	}
}
